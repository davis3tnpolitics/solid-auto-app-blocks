{
  "name": "solid-auto-app-blocks",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "create:block": "node automations/scripts/create-block.mjs",
    "create:workflow": "node automations/scripts/create-workflow.mjs",
    "create:next-app": "node automations/generators/next-app.js",
    "create:nest-app": "node automations/generators/nest-app.js",
    "add:auth": "node automations/generators/front-end/add-ons/add-auth.js",
    "update:api": "node automations/generators/api-updator.js",
    "gen:examples": "node automations/scripts/gen-examples.mjs",
    "dev": "pnpm -r --parallel --filter \"./apps/*\" --if-present dev",
    "build": "pnpm -r --filter \"./packages/*\" --filter \"./apps/*\" --if-present build",
    "lint:workspace": "node automations/scripts/lint-workspace.mjs",
    "lint:automation-contracts": "node automations/scripts/lint-automation-contracts.mjs",
    "lint": "pnpm lint:workspace && pnpm lint:automation-contracts",
    "test:automations": "pnpm --filter automations-tests test",
    "test:automations:update": "pnpm --filter automations-tests test:update",
    "typecheck": "pnpm -r --filter \"./packages/*\" --filter \"./apps/*\" --if-present typecheck",
    "typecheck:ui": "pnpm --filter @workspace/ui typecheck",
    "test:ui": "pnpm --filter @workspace/ui test:coverage",
    "test": "pnpm test:automations && pnpm test:ui",
    "verify": "pnpm lint && pnpm typecheck && pnpm test"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.28.0"
}
