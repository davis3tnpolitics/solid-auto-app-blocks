{
  "name": "database",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "db:generate": "prisma generate --schema ./prisma",
    "db:migrate": "prisma migrate deploy --schema ./prisma",
    "db:migrate:dev": "prisma migrate dev --schema ./prisma",
    "db:migrate:create-only": "prisma migrate dev --schema ./prisma --create-only --name profile_table",
    "db:migrate:reset": "prisma migrate reset --schema ./prisma",
    "db:format": "prisma format --schema ./prisma",
    "db:push": "prisma db push --schema ./prisma",
    "db:studio": "prisma studio --schema ./prisma"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.28.0",
  "dependencies": {
    "@prisma/client": "^6.19.1",
    "class-validator": "^0.14.1",
    "prisma": "^6.19.1"
  },
  "devDependencies": {
    "@types/node": "^25.0.6",
    "prisma-class-validator-generator": "^6.2.0",
    "prisma-generator-fake-data": "^0.18.2",
    "prisma-markdown": "2.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
